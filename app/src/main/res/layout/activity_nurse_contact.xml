        android:layout_marginBottom="468dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/illustration" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="330dp"
        android:layout_height="117dp"
        android:layout_marginTop="16dp"
        android:text="Dr. Stella is the top most heart surgeon in Flower  Hospital. She has done over 100 successful sugeries  within past 3 years. She has achieved several awards for her wonderful contribution in her own  field. Sheâ€™s available for private consultation for given schedules."
        android:textColor="#6B6996"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
